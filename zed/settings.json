{
	"agent_servers": {
		"qwen-code": {
			"type": "registry"
		},
		"opencode": {
			"type": "registry"
		}
	},
	"inlay_hints": {
		"enabled": false
	},
	"format_on_save": "on",
	"vim": {
		"use_system_clipboard": "on_yank",
		"default_mode": "normal",
		"highlight_on_yank_duration": 150
	},
	"file_scan_exclusions": [
		"**/.git",
		"**/.svn",
		"**/.hg",
		"**/.jj",
		"**/CVS",
		"**/.DS_Store",
		"**/Thumbs.db",
		"**/.classpath",
		"**/.project",
		"**/.idea",
		"**/.vscode",
		"**/.astro",
		"**/.gradle",
		"**/.m2",
		"**/.next",
		"**/.turbo",
		"**/.vercel",
		"**/.nyc_output",
		"**/coverage",
		"**/node_modules",
		"**/node_modules/**",
		"**/.vscode-test",
		"**/__pycache__",
		"**/.mypy_cache",
		"**/.ruff_cache",
		"**/.venv",
		"**/venv",
		"**/build",
		"**/dist",
		"**/target",
		"**/.expo",
		"**/.expo-shared",
		"**/ios/Pods",
		"**/android/.gradle",
		"**/android/build",
		"**/migrations",
		"**/*.pyc",
		"**/db.sqlite",
		"**/*.db"
	],
	"file_types": {
		"dotenv": [".env*"],
		"jsonc": ["**/frontmatter.json", "**/.frontmatter/config/*.json"]
	},
	"autosave": {
		"after_delay": {
			"milliseconds": 1000
		}
	},
	"soft_wrap": "editor_width",
	"buffer_font_family": "CaskaydiaCove Nerd Font",
	"buffer_font_size": 16,
	"buffer_font_features": {
		"ligatures": true,
		"calt": true,
		"cv01": true
	},
	"ui_font_size": 16,
	"hide_mouse": "on_typing_and_movement",
	"cursor_blink": false,
	"relative_line_numbers": "enabled",
	"show_wrap_guides": false,
	"indent_guides": {
		"line_width": 1,
		"active_line_width": 2,
		"coloring": "indent_aware",
		"background_coloring": "disabled"
	},
	"vim_mode": true,
	"base_keymap": "VSCode",
	"use_on_type_format": false,
	"search": {
		"case_sensitive": false,
		"include_ignored": false,
		"whole_word": false,
		"button": true,
		"center_on_match": true,
		"regex": false
	},
	"use_smartcase_search": true,
	"completions": {
		"words_min_length": 3
	},
	"show_signature_help_after_edits": true,
	"show_completions_on_input": true,
	"colorize_brackets": true,
	"show_whitespaces": "none",
	"toolbar": {
		"breadcrumbs": false,
		"quick_actions": false
	},
	"project_panel": {
		"hide_gitignore": false,
		"entry_spacing": "standard",
		"indent_guides": {
			"show": "always"
		},
		"dock": "right",
		"auto_reveal_entries": true
	},
	"tabs": {
		"file_icons": true,
		"git_status": true
	},
	"which_key": {
		"enabled": true
	},
	"icon_theme": "VSCode Icons for Zed (Dark)",
	"git": {
		"inline_blame": {
			"enabled": false
		},
		"git_gutter": "tracked_files"
	},
	"context_servers": {},
	"agent": {
		"dock": "left",
		"play_sound_when_agent_done": true,
		"default_profile": "write",
		"always_allow_tool_actions": true,
		"default_model": {
			"provider": "google",
			"model": "gemini-3-flash-preview"
		},
		"model_parameters": []
	},
	"wsl_connections": [
		{
			"distro_name": "Ubuntu-24.04",
			"user": "vikiru",
			"projects": [
				{
					"paths": ["/home/vikiru/projects"]
				}
			]
		},
		{
			"distro_name": "Ubuntu-24.04",
			"user": null,
			"projects": []
		}
	],
	"features": {
		"edit_prediction_provider": "zed"
	},
	"auto_update_extensions": {
		"opencode": false
	},
	"telemetry": {
		"diagnostics": false,
		"metrics": false
	},
	"redact_private_values": true,
	"theme": {
		"mode": "system",
		"light": "One Dark",
		"dark": "One Dark"
	},
	"notification_panel": {
		"dock": "left"
	},
	"git_panel": {
		"tree_view": true,
		"dock": "right"
	},
	"outline_panel": {
		"dock": "right"
	},
	"auto_install_extensions": {
		"git-firefly": true,
		"dockerfile": true,
		"docker-compose": true,
		"lua": true,
		"html": true,
		"astro": true,
		"tombi": true,
		"tsgo": true,
		"biome": true,
		"ruff": true,
		"java-eclipse-jdtls": true,
		"yaml": true,
		"sql": true,
		"markdown-oxide": true
	},
	"terminal": {
		"working_directory": "current_project_directory",
		"font_family": "JetBrainsMono Nerd Font",
		"font_size": 15,
		"copy_on_select": true,
		"detect_venv": {
			"on": {
				"directories": [".env", "env", ".venv", "venv"],
				"activate_script": "default"
			}
		}
	},
	"minimap": {
		"show": "never"
	},
	"scrollbar": {
		"show": "never"
	},
	"sticky_scroll": {
		"enabled": true
	},
	"vertical_scroll_margin": 0,
	"scroll_beyond_last_line": "off",
	"active_pane_modifiers": {
		"inactive_opacity": 0.4
	},
	"preview_tabs": {
		"enabled": true,
		"enable_preview_from_file_finder": true,
		"enable_preview_from_project_panel": true,
		"enable_preview_from_multibuffer": true,
		"enable_preview_file_from_code_navigation": true
	},
	"use_system_path_prompts": false,
