					"classRegex": [
						["@apply\\s+([^;\\n]+)", "\\b([a-z-]+(?:-[a-z-]+)*)\\b"],
						"class\\s*=\\s*\"([^\"]*)\"",
						"clx\\s*=\\s*\"([^\"]*)\"",
						["html`([^`]*)`", "class\\s*=\\s*\"([^\"]*)\""],
						["html`([^`]*)`", "class\\s*=\\s*'([^']*)'"],
						["innerHTML\\s*=\\s*`([^`]*)`", "class\\s*=\\s*\"([^\"]*)\""],
						["clx\\s*\\(\\s*\\{([^}]*)\\}\\s*\\)"],
						["cn\\s*\\(\\s*\\{([^}]*)\\}\\s*\\)"]
					]
				}
			}
		},
		"ruff": {
			"initialization_options": {
				"settings": {
					"format": {
						"enabled": true
					},
					"linter": {
						"enabled": true
					},
					"codeActions": {
						"source": {
							"organizeImports": {
								"enabled": true
							}
						}
					}
				}
			}
		},
		"lua-language-server": {
			"settings": {
				"Lua": {
					"diagnostics": {
						"globals": ["vim"]
					}
				}
			}
		}
	},
