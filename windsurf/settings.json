{
    "extensions.autoUpdate": false,
    "extensions.autoCheckUpdates": false,
    "telemetry.feedback.enabled": false,
    "telemetry.telemetryLevel": "off",
    "mdb.sendTelemetry": false,
    "telemetry.editStats.enabled": false,
    "telemetry.editStats.details.enabled": false,
    "telemetry.editStats.showDecorations": false,
    "telemetry.editStats.showStatusBar": false,
    "redhat.telemetry.enabled": false,
    "editor.experimental.treeSitterTelemetry": false,
    "npm.packageManager": "pnpm",
    "remote.autoForwardPortsSource": "hybrid",
    "workbench.colorTheme": "Aura Dark (Soft Text)",
    "workbench.sideBar.location": "right",
    "workbench.iconTheme": "vscode-icons",
    "workbench.list.smoothScrolling": true,
    "editor.fontFamily": "JetBrains Mono, Consolas, 'Courier New', monospace",
    "editor.fontLigatures": "'calt', 'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'ss07', 'ss08', 'ss09', 'ss10', 'liga'",
    "editor.cursorBlinking": "smooth",
    "editor.minimap.enabled": false,
    "editor.formatOnSave": true,
    "editor.detectIndentation": true,
    "editor.wordWrap": "on",
    "editor.linkedEditing": true,
    "editor.stickyScroll.enabled": true,
    "explorer.compactFolders": false,
    "editor.accessibilitySupport": "off",
    "editor.guides.bracketPairs": "active",
    "editor.guides.indentation": true,
    "editor.bracketPairColorization.enabled": true,
    "editor.renderWhitespace": "none",
    "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
    "editor.suggest.showSnippets": true,
    "editor.snippetSuggestions": "inline",
    "editor.suggest.filterGraceful": true,
    "editor.suggestSelection": "recentlyUsedByPrefix",
    "editor.suggest.insertMode": "replace",
    "editor.suggest.localityBonus": true,
    "editor.suggest.matchOnWordStartOnly": false,
    "editor.suggest.shareSuggestSelections": true,
    "editor.suggestOnTriggerCharacters": true,
    "editor.suggest.preview": false,
    "editor.wordBasedSuggestions": "matchingDocuments",
    "editor.suggest.showWords": false,
    "editor.showDeprecated": false,
    "editor.quickSuggestions": {
        "other": true,
        "comments": false,
        "strings": true
    },
    "terminal.integrated.defaultProfile.linux": "zsh",
    "files.autoSave": "afterDelay",
    "files.encoding": "utf8",
    "files.eol": "\n",
    "search.useIgnoreFiles": true,
    "search.useParentIgnoreFiles": true,
    "search.useGlobalIgnoreFiles": true,
    "explorer.excludeGitIgnore": false,
    "explorer.fileNesting.enabled": true,
    "explorer.fileNesting.patterns": {
        "package.json": "astro.config.*, pnpm-lock.yaml, pnpm-workspace.yaml, package-lock.json, yarn.lock, bun.lockb, .npmrc, .yarnrc*, pnpm-debug.log, biome.*, tailwind.config.*, postcss.config.*, vite.config.*, tsconfig.*, eslint.*, prettier.*, .eslintrc*, .prettierrc*, commitlint.*, lefthook.*, playwright.config.*, vitest.config.*, env.d.ts, knip.*, .releaserc*, .lintstagedrc*",
        "pyproject.toml": "uv.lock, ruff.toml, .ruff_cache, .pytest_cache, requirements.txt, setup.py, setup.cfg, .python-version",
        "pom.xml": "mvnw, mvnw.cmd, .mvn, target, checkstyle.xml, lombok.config, .factorypath",
        "build.gradle": "settings.gradle, gradlew, gradlew.bat, checkstyle.xml, lombok.config",
        "*.ts": "${capture}.test.ts, ${capture}.spec.ts, ${capture}.d.ts",
        "*.tsx": "${capture}.test.tsx, ${capture}.spec.tsx, ${capture}.module.css, ${capture}.css, ${capture}.stories.tsx, ${capture}.d.ts",
        "*.js": "${capture}.test.js, ${capture}.spec.js, ${capture}.js.map",
        "*.jsx": "${capture}.test.jsx, ${capture}.spec.jsx, ${capture}.module.css, ${capture}.css",
        "*.astro": "${capture}.test.ts, ${capture}.css, ${capture}.d.ts",
        "*.html": "${capture}.css, ${capture}.js",
        ".env": ".env.*, .envrc, env.d.ts"
    },
    "diffEditor.codeLens": true,
    "diffEditor.ignoreTrimWhitespace": true,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/node_modules": true,
        "**/node_modules/**": true,
        "**/.vscode-test": true,
        "**/__pycache__": true,
        "**/.pytest_cache": true,
        "**/.ruff_cache": true,
        "**/.venv": true,
        "**/venv": true,
        "**/*.code-search": true
    },
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/**": true,
        "**/.astro/**": true,
        "**/dist/**": true,
        "**/build/**": true,
        "**/node_modules": true,
        "**/__pycache__/**": true,
        "**/.venv/**": true,
        "**/venv/**": true,
        "**/.ruff_cache/**": true,
        "**/.pytest_cache/**": true,
        "**/.mypy_cache/**": true
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/dist": true,
        "**/build": true,
        "**/.cache": true,
        "**/*.code-search": true
    },
    "typescript.tsserver.experimental.enableProjectDiagnostics": false,
    "typescript.experimental.useTsgo": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "javascript.preferences.importModuleSpecifier": "non-relative",
    "typescript.preferences.importModuleSpecifier": "non-relative",
    "typescript.preferences.includePackageJsonAutoImports": "off",
    "typescript.preferences.quoteStyle": "single",
    "typescript.workspaceSymbols.scope": "currentProject",
    "javascript.preferences.quoteStyle": "single",
    "python.languageServer": "None",
    "astro.content-intellisense": true,
    "java.import.gradle.enabled": false,
    "java.import.maven.enabled": true,
    "java.format.enabled": true,
    "java.saveActions.organizeImports": true,
    "java.maxConcurrentBuilds": 1,
    "java.errors.incompleteClasspath.severity": "ignore",
    "[css]": {
        "editor.defaultFormatter": "vscode.css-language-features"
    },
    "[javascript]": {
        "editor.defaultFormatter": "biomejs.biome"
    },
    "[typescript]": {
        "editor.defaultFormatter": "biomejs.biome"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "biomejs.biome"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[python]": {
        "editor.defaultFormatter": "charliermarsh.ruff"
    },
    "[astro]": {
        "editor.defaultFormatter": "astro-build.astro-vscode"
    }
}